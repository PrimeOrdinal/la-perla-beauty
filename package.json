  "scripts": {
    "build:gatsby": "gatsby build",
    "build:lambda": "netlify-lambda build .netlify/functions/src",
    "build-storybook": "NODE_ENV=production build-storybook",
    "build": "npm run build:gatsby && npm run build:lambda",
    "chromatic": "npx chromatic --project-token=dzp7tbz8xd7",
    "clean": "gatsby clean",
    "copy-static-queries": "mkdir -p public/static/d/ && cp -R public/page-data/sq/d/. public/static/d/",
    "develop": "gatsby develop -p 8000",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve",
    "format": "prettier --write \"**/*.{cjs,js,json,mjs,ts,tsx}\"",
    "lint:css": "stylelint './src/**/*.{cjs,js,mjs,ts,tsx}'",
    "lint:css:fix": "npm run lint:css --",
    "lint:js": "eslint --config=eslint-config.js '*/**/*.{cjs,js,json,mjs,ts,tsx}'",
    "lint:js:fix": "npm run lint:js -- --fix",
    "postinstall": "netlify-lambda install .netlify/functions/src",
    "serve": "gatsby serve",
    "start": "netlify dev",
    "storybook": "NODE_ENV=production start-storybook -s ./static -p 6006",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "type-check": "tsc --noEmit"
