{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@date-io/dayjs": "2.10.6",
    "@mapbox/geo-viewport": "0.4.1",
    "@turf/turf": "5.1.6",
    "clsx": "1.1.1",
    "dayjs": "1.9.7",
    "formik": "2.2.6",
    "gatsby": "2.29.1",
    "gatsby-image": "2.8.0",
    "gatsby-image-background-slider": "0.0.4",
    "gatsby-plugin-feed": "2.10.0",
    "gatsby-plugin-google-tagmanager": "2.8.0",
    "gatsby-plugin-manifest": "2.9.0",
    "gatsby-plugin-offline": "3.7.0",
    "gatsby-plugin-purgecss": "5.0.0",
    "gatsby-plugin-react-helmet": "3.7.0",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-robots-txt": "1.5.5",
    "gatsby-plugin-sharp": "2.11.1",
    "gatsby-source-bigcommerce": "0.3.7",
    "gatsby-source-contentstack": "2.3.1",
    "gatsby-source-filesystem": "2.8.0",
    "gatsby-transformer-sharp": "2.9.0",
    "graphql-iso-date": "3.6.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-optional-chaining": "7.12.7",
    "@babel/preset-typescript": "7.12.7",
    "@mapbox/togeojson": "0.16.0",
    "@types/jest": "26.0.19",
    "@types/node": "14.14.14",
    "@types/prop-types": "15.7.3",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-helmet": "6.1.0",
    "@types/react-test-renderer": "17.0.0",
    "@types/request": "2.48.5",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "babel-jest": "26.6.3",
    "babel-plugin-import": "1.13.3",
    "babel-plugin-styled-components": "1.12.0",
    "babel-preset-gatsby": "0.9.0",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-react": "7.21.5",
    "graphql-schema-linter": "2.0.1",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "react-test-renderer": "17.0.1",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3"
  },
  "graphql-schema-linter": {
    "rules": [
      "enum-values-sorted-alphabetically"
    ]
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged",
      "pre-push": "npm run-script test"
    }
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "lint-staged": {
    "*.{js,ts}": [
      "git add",
      "npm run-script lint-code"
    ],
    "*.graphql": [
      "npm run-script lint-schema"
    ]
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint-code": "eslint . --ext .js,.jsx,.ts,.tsx",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
